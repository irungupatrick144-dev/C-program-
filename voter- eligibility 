#include <stdio.h>
int main(){
	int age;
	//prompt the user to enter their age
	printf("enter your age\n");
	scanf("%d",& age);
	if (age >=18){
	
		printf("you are eligible to vote");
	}
		else{
			printf("you are not eligible to vote");
		}
	return 0;
}